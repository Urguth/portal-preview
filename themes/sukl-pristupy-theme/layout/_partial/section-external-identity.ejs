<section id="<%- site.data.content_cz[count].tag %>" class="section section-external-identity background-config">
    <div class="section-table section-margin">
        <h2 class="section__title white-color"><%- site.data.content_cz[count].title %></h2>
        <div class="section__line-container">
            <div class="section__line-1"></div>
            <div class="section__line-2"></div>
        </div>
        <p class="section__description white-color"><%- site.data.content_cz[count].description %></p>
        <div class="section-tabs">
            <% for (var i in site.data.content_cz[count].content){ %>
            <div class="row-3 section-tab">
                <a href="<%- site.data.content_cz[count].content[i].url %>" target="<%- site.data.content_cz[count].content[i].target %>">
                    <div class="section-tab__table">
                        <div class="section-tab__table-inner">
                            <i class="fa <%- site.data.content_cz[count].content[i].logo %> fa-3x section-tab__icon"
                               aria-hidden="true"></i>
                            <i class="fa <%- site.data.content_cz[count].content[i].pictogram %> section-tab__icon section-tab__icon-type"
                               aria-hidden="true"></i>
                            <h2 class="section-tab__title"><%- site.data.content_cz[count].content[i].title %></h2>
                            <span class="section-tab__description"><%- site.data.content_cz[count].content[i].description %></span>
                        </div>
                    </div>
                </a>
            </div>
            <% } %>
        </div>
    </div>
</section>