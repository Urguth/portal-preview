<section id="<%- site.data.content_cz[0].tag %>" class="section section-information">
    <div class="section-symbol section-symbol--white">
        <img class="section-symbol__logo" src="./img/<%- site.data.content_cz[0].sectionLogo %>" alt="information-image">
    </div>
    <div class="section-table section-margin">
        <h2 class="section__title"><%- site.data.content_cz[0].title %></h2>
        <div class="section__line-container">
            <div class="section__line-1"></div>
            <div class="section__line-2"></div>
        </div>
        <p class="section__description"><%- site.data.content_cz[0].description %></p>
        <div class="section-tabs">
            <% for (var i in site.data.content_cz[0].content){ %>
                <div class="row-3 section-tab">
                    <a href="<%- site.data.content_cz[0].content[i].url %>" target="<%- site.data.content_cz[0].content[i].target %>">
                        <div class="section-tab__table">
                            <div class="section-tab__table-inner">
                                <i class="fa <%- site.data.content_cz[0].content[i].logo %> fa-3x section-tab__icon"
                                   aria-hidden="true"></i>
                                <i class="fa <%- site.data.content_cz[0].content[i].pictogram %> section-tab__icon section-tab__icon-type"
                                   aria-hidden="true"></i>
                                <div class="section-tab__title"><%- site.data.content_cz[0].content[i].title %></div>
                            </div>
                        </div>
                    </a>
                </div>
            <% } %>
        </div>
    </div>
</section>