<section id="<%- jsonName[count].tag %>" class="section section-request">
    <div class="section-table section-margin">
        <h2 class="section__title white-color"><%- jsonName[count].title %></h2>
        <div class="section__line-container">
            <div class="section__line-1"></div>
            <div class="section__line-2"></div>
        </div>
        <p class="section__description white-color"><%- jsonName[count].description %></p>
        <div class="section-tabs">
            <% for (var i in jsonName[count].content){ %>
            <div class="row-3 section-tab">
                <div class="section-tab__foreground-content">
                    <% if(jsonName[count].content[i].doubleUrl) { %>
                    <% } else { %>
                    <a href="<%- jsonName[count].content[i].url1 %>" target="<%- jsonName[count].content[i].target %>">
                        <i class="fa <%- jsonName[count].content[i].pictogram1 %> fa-3x section-tab__foreground-icon" aria-hidden="true"></i>
                    </a>
                    <% } %>
                </div>
                <div class="section-tab__table">
                    <div class="section-tab__table-inner">
                        <i class="fa <%- jsonName[count].content[i].logo %> fa-3x section-tab__icon"
                           aria-hidden="true"></i>
                        <div class="section-tab__title"><%- jsonName[count].content[i].title %></div>
                        <div class="section-tab__spen"></div>
                        <div class="section-tab__description"><%- jsonName[count].content[i].description %></div>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
    </div>
</section>